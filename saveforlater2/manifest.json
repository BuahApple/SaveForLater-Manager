{
  "name": "SaveForLater Manager",
  "version": "0.2",
  "description": "Download all webpage on current window instantly!",
  "icons":{"16":"favicon-16x16.png",
		   "48":"favicon-32x32.png",
		   "128":"favicon-150x150.png"},
